{
  "name": "Draw-Circle",
  "version": "1.1.0",
  "description": "Demo of drawing geofence circles with Mapbox GL JS API",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js:bundle.js --live -- --debug --delay=0",
    "build": "watchify index.js -o bundle.js --debug --delay=0 -v",
    "dist": "time browserify -t [ babelify --presets [ es2015 ] ] index.js | uglifyjs -c -m > bundle.min.js",
    "test": "eslint ."
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@turf/bbox": "^4.5.2",
    "@turf/bbox-polygon": "^4.5.2",
    "@turf/circle": "^4.5.2",
    "@turf/destination": "^4.5.2",
    "@turf/distance": "^4.5.2",
    "@turf/helpers": "^4.5.2",
    "@turf/truncate": "^4.5.2",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "mapbox-gl": "^0.38.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "uglify-js": "^3.0.24",
    "watchify": "^3.9.0"
  },
  "optionalDependencies": {
    "fsevents": "^1.1.2"
  }
}
