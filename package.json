{
  "name": "Draw-Circle",
  "version": "1.0.0",
  "description": "Demo of drawing geofence circles with Mapbox GL JS API",
  "main": "index.js",
  "scripts": {
    "start": "budo index.js:build/bundle.js --live -- --debug --delay=0",
    "build": "mkdir -p build && watchify index.js -o build/bundle.js --debug --delay=0 -v",
    "dist": "mkdir -p dist && time browserify -t [ babelify --presets [ es2015 ] ] index.js | uglifyjs -c -m > dist/bundle.min.js",
    "test": "eslint ."
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "dependencies": {
    "@turf/bbox": "^4.5.2",
    "@turf/bbox-polygon": "^4.5.2",
    "@turf/truncate": "^4.5.2",
    "fsevents": "^1.1.2",
    "lodash": "^4.17.4",
    "lodash.debounce": "^4.0.8",
    "mapbox-gl": "^0.38.0",
    "turf": "^3.0.14",
    "turf-bbox": "^3.0.12",
    "turf-bbox-polygon": "^3.0.12",
    "turf-circle": "^3.0.12",
    "turf-helpers": "^3.0.12",
    "turf-inside": "^3.0.12"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "uglify-js": "^3.0.24",
    "watchify": "^3.9.0"
  }
}
